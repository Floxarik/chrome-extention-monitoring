{
  "manifest_version": 3,
  "name": "Extension Monitor",
  "version": "1.2",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["management", "storage"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
